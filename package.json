    "@tailwindcss/ui": "^0.6.0",
    "@testing-library/vue": "^5.0.4",
    "@types/debounce": "^1.2.0",
    "@types/node": "^14.6.4",
    "@vue/compiler-sfc": "^3.0.0-rc.10",
    "@vue/test-utils": "^2.0.0-beta.3",
    "husky": "^4.3.0",
    "tailwindcss": "^1.8.6",
    "tsdx": "^0.13.3",
    "vite": "^1.0.0-rc.4",
    "vue": "^3.0.0-rc.10"
  },
  "dependencies": {
    "debounce": "^1.2.0"
  }
